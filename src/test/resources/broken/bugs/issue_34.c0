void append(Node *root, int value)
  //@requires ?;
  //@ensures ? && list(root);
{
  Node *n = root;
  while (n->next != NULL)
    //@loop_invariant ?;
    n = n->next;
  n->next = alloc(Node);
  n->next->value = value;
}

